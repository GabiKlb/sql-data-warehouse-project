CREATE TABLE bronze.prd_info (
	prd_id INTEGER,
	prd_key VARCHAR(50),
	prd_nm VARCHAR(100),
	prd_cost INTEGER,
	prd_line VARCHAR(50),
	prd_start_dt DATE,
	prd_end_dt DATE
);

CREATE TABLE bronze.sales_details (
	sls_ord_num VARCHAR(100),
	sls_prd_key VARCHAR(100),
	sls_cust_id INTEGER,
	sls_order_dt INTEGER,
	sls_ship_dt INTEGER,
	sls_due_dt INTEGER,
	sls_sales INTEGER,
	sls_quantity INTEGER,
	sls_price INTEGER
);

CREATE TABLE crm_cust_info (
	cst_id INTEGER,
	cst_key VARCHAR(50),
	cst_firstname VARCHAR(50),
	cst_lastname VARCHAR(50),
	cst_marital_status VARCHAR(50),
	cst_gndr VARCHAR(50),
	cst_create_Date DATE
);

CREATE TABLE bronze.erp_loc_a101 (
	cid VARCHAR(100),
	cntry VARCHAR(100)
);

CREATE TABLE bronze.erp_cust_az12 (
	cid VARCHAR(100),
	bdate DATE,
	gen VARCHAR(100)
);

CREATE TABLE bronze.erp_px_cat_g1v2 (
	id VARCHAR(100),
	cat VARCHAR(100),
	subcat VARCHAR(100),
	maintenance VARCHAR(100)
);
